#!/bin/sh /etc/rc.common
# Copyright (C) 2006 OpenWrt.org
START=61

USE_PROCD=1
PROG=saned

start_service() {
	procd_open_instance
	procd_set_param command "$PROG"
	procd_append_param command -a
	procd_set_param respawn
	procd_close_instance
}

stop_service() {
	kill $(ps | grep $PROG | grep -v grep | awk '{print $1}')
}
